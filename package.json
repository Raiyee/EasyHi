{
  "name": "easyhi-vue-ssr",
  "description": "refactor EasyHi using vue, vuex, vue-router and ssr",
  "author": "liujiaren1992@vip.qq.com",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node server",
    "start": "cross-env NODE_ENV=production DEBUG=hi:* node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production DEBUG=hi:* webpack --config build/webpack/config.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production DEBUG=hi:* webpack --config build/webpack/config.server.js --progress --hide-modules"
  },
