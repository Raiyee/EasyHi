{
  "name": "easyhi-vue",
  "version": "0.0.1",
  "description": "refactor EasyHi using vue, vuex, vue-router",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@git.coding.net:Raiyee/EasyHi.git"
  },
  "scripts": {
    "build": "better-npm-run build",
    "build:test": "better-npm-run build:test",
    "clean": "better-npm-run clean",
    "compile": "better-npm-run compile",
    "compile:test": "better-npm-run compile:test",
    "debug": "better-npm-run dev -- --debug",
    "dev": "better-npm-run dev",
    "dev:mock": "better-npm-run dev -- --mock",
    "eslint": "eslint --max-warnings 10 --ext .js,.vue .",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix && npm run stylelint",
    "start": "better-npm-run start",
    "start:test": "better-npm-run start:test",
    "stylelint": "better-npm-run stylelint",
    "test": "better-npm-run test",
    "push": "bash build/bash/push.sh",
    "sync": "bash build/bash/sync.sh"
  },
  "betterScripts": {
    "build": {
      "command": "npm run lint:fix && npm run clean && npm run compile",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "build:test": {
      "command": "npm run lint:fix && npm run clean && npm run compile:test",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "clean": {
      "command": "node build/bin/clean",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "compile": {
      "command": "node build/bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "compile:test": {
      "command": "node build/bin/compile",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "koa*"
      }
    },
    "dev": {
      "command": "nodemon build/bin/start",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "start": {
      "command": "nodemon build/bin/start",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "start:test": {
      "command": "nodemon build/bin/start",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "koa*"
      }
    },
    "stylelint": {
      "command": "node build/bin/stylelint.js",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "test": {
      "command": "npm run compile:test && npm run start:test"
    }
  },
  "dependencies": {
    "axios": "^0.15.2",
    "bootstrap-styl": "^5.0.8",
    "fonts.css": "^1.5.0",
    "moment": "^2.15.2",
    "vue": "^2.0.5",
    "vue-lazyload": "^0.8.3",
    "vue-router": "^2.0.1",
    "vue-touch": "vuejs/vue-touch#next",
    "vuex": "^2.0.0",
    "vuex-actions": "^1.1.1",
    "vuex-localstorage": "^0.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "better-npm-run": "^0.0.13",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "debug": "^2.2.0",
    "del": "^2.2.2",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "eslint-plugin-vuefix": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "koa": "^2.0.0-alpha.7",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-static": "^3.0.0",
    "mockjs": "^1.0.1-beta3",
    "nib": "^1.1.2",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "stylint": "^1.5.7",
    "stylint-stylish": "^1.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "stylus-px2rem": "^1.0.6",
    "url-loader": "^0.5.7",
    "vconsole": "^2.5.1",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.8.0",
    "vue-promise-loader": "^9.8.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "yargs": "^6.3.0"
  }
}
