{
  "name": "easyhi-vue",
  "version": "0.0.1",
  "description": "refactor EasyHi using vue, vuex, vue-router",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@git.coding.net:Raiyee/EasyHi.git"
  },
  "scripts": {
    "gfw": "yarn add -D cross-env && cross-env PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs yarn",
    "build": "npm run lint:fix && npm run unit && npm run clean && npm run compile",
    "build:test": "npm run lint:fix && npm run clean && npm run compile:test",
    "clean": "cross-env DEBUG=koa* node build/bin/clean",
    "compile": "cross-env NODE_ENV=production DEBUG=koa* node build/bin/compile",
    "compile:test": "cross-env NODE_ENV=test DEBUG=koa* node build/bin/compile",
    "debug": "npm run dev -- --debug",
    "dev": "cross-env DEBUG=koa* nodemon build/bin/start",
    "dev:mock": "nodemon build/bin/start -- --mock",
    "eslint": "eslint --max-warnings 10 --ext .js,.vue .",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix && npm run stylelint",
    "prod": "npm run compile && npm run start",
    "start": "cross-env NODE_ENV=production DEBUG=koa* node build/bin/start",
    "start:test": "cross-env NODE_ENV=test nodemon build/bin/start",
    "stylelint": "node build/bin/stylelint.js",
    "test": "npm run compile:test && npm run start:test",
    "push": "bash build/bash/push.sh",
    "sync": "bash build/bash/sync.sh",
    "unit": "cross-env NODE_ENV=testing DEBUG=koa* karma start ./test/unit/runner.js",
    "unit:dev": "npm run unit -- --watch",
    "coveralls": "cat ./coverage/*/lcov.info | coveralls",
    "codecov": "cat ./coverage/*/lcov.info | codecov"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "bootstrap-styl": "^5.0.8",
    "fonts.css": "^1.5.0",
    "moment": "^2.17.0",
    "v-touch": "^0.1.9",
    "vue": "^2.1.3",
    "vue-lazyload": "^0.9.3",
    "vue-router": "^2.0.3",
    "vue-touch-easyhi": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-actions": "^1.1.1",
    "vuex-localstorage": "^0.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.25.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "cssnano": "^3.8.1",
    "debug": "^2.3.3",
    "del": "^2.2.2",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "eslint-plugin-vuefix": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^1.3.0",
