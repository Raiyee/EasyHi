{
  "name": "easyhi-vue-ssr",
  "description": "refactor EasyHi using vue, vuex, vue-router and ssr",
  "author": "liujiaren1992@vip.qq.com",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production DEBUG=hi:* webpack --config build/webpack/config.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production DEBUG=hi:* webpack --config build/webpack/config.server.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.24.1",
    "lru-cache": "^4.0.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "vue": "^2.0.0",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "buble": "^0.14.2",
    "buble-loader": "^0.3.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "memory-fs": "^0.3.0",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.2",
    "sw-precache-webpack-plugin": "^0.6.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.9.3",
    "webpack": "^2.1.0-beta.26",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
