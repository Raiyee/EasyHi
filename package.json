{
  "name": "easyhi-vue",
  "version": "2.0.0-beta.2",
  "description": "refactor EasyHi using vue, vuex, vue-router",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:mock": "better-npm-run dev -- --mock",
    "debug": "better-npm-run debug",
    "lint": "eslint --max-warnings 10 --ext .js,.vue .",
    "lint:fix": "npm run lint -- --fix",
    "clean": "better-npm-run clean",
    "compile": "better-npm-run compile",
    "build": "better-npm-run build",
    "push": "bash build/bash/push.sh"
  },
  "betterScripts": {
    "compile": {
      "command": "node build/bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "start": {
      "command": "nodemon build/bin/start",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "dev": {
      "command": "nodemon build/bin/start",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "debug": {
      "command": "nodemon --debug build/bin/start",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "clean": {
      "command": "node build/bin/clean",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "build": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "fonts.css": "^1.4.0",
    "lesshat": "^4.1.0",
    "normalize.css": "^4.2.0",
    "vue": "^2.0.0-rc.1",
    "vue-router": "^2.0.0-rc.3",
    "vuex": "^2.0.0-rc.4",
    "vuex-localstorage": "^0.2.0",
    "vuex-promise": "^2.0.0-rc.3"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.10",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "copy-webpack-plugin": "^3.0.1",
    "cross-spawn": "^4.0.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "eslint-plugin-vue": "^0.1.1",
    "eslint-plugin-vuefix": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "github-markdown-css": "^2.3.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0",
    "koa-error": "^2.1.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-send": "^3.2.0",
    "koa-static": "^3.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "marked": "^0.3.5",
    "nightwatch": "^0.9.5",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "normalize.css": "^4.2.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "selenium-server": "^2.53.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.5",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.3.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "yargs": "^4.8.1"
  },
  "engines": {
    "node": ">= 5.10.0 <= 5.12.0",
    "npm": ">= 3.6.0 <= 3.8.6"
  }
}
