div(:class="classes.pickers")
  div(v-if="pickerTitle", :class="classes.pickersTitle") {{ pickerTitle }}
  template(v-for="(pickerListData, index) of pickerList")
    picker-list(v-bind="pickerListData",
    :index="index",
    :changingIndex="changingIndex",
    :maxWidth="maxWidth",
    :hasTitle="!!hasTitle",
    :mask="pickerMask",
    :visibleCount="visibleCount",
    @itemChanged="itemChanged")
    div(v-if="pickerDivider && index !== pickerList.length - 1", :class="[classes.pickerDivider, {[classes.hasTitle]: hasTitle}]")
